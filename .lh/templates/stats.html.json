            "content": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    {% load static %}\r\n  <meta charset=\"utf-8\">\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\r\n  <title>statis</title>\r\n\r\n  <!-- Google Font: Source Sans Pro -->\r\n  <link rel=\"stylesheet\" href=\"https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback\">\r\n  <!-- Font Awesome Icons -->\r\n  <link rel=\"stylesheet\" href=\"{% static 'plugins/fontawesome-free/css/all.min.css' %}\">\r\n  <!-- IonIcons -->\r\n  <link rel=\"stylesheet\" href=\"https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css\">\r\n  <!-- Theme style -->\r\n  <link rel=\"stylesheet\" href=\"{% static 'dist/css/adminlte.min.css' %}\">\r\n\r\n  <link rel=\"stylesheet\" href=\"https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css\">\r\n  <script src=\"https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js\"></script>\r\n  <script src=\"https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js\"></script>\r\n  <script src=\"https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js\"></script>\r\n\r\n</head>\r\n<!--\r\n`body` tag options:\r\n\r\n  Apply one or more of the following classes to to the body tag\r\n  to get the desired effect\r\n\r\n  * sidebar-collapse\r\n  * sidebar-mini\r\n-->\r\n<body class=\"hold-transition sidebar-mini\">\r\n\r\n<div class=\"wrapper\">\r\n  <!-- Navbar -->\r\n  <nav class=\"main-header navbar navbar-expand navbar-white navbar-light\">\r\n    <!-- Left navbar links -->\r\n    <ul class=\"navbar-nav\">\r\n      <li class=\"nav-item\">\r\n        <a class=\"nav-link\" data-widget=\"pushmenu\" href=\"#\" role=\"button\"><i class=\"fas fa-bars\"></i></a>\r\n      </li>\r\n      <li class=\"nav-item d-none d-sm-inline-block\">\r\n        <a href=\"{% url 'stats' %}\" class=\"nav-link active\">Home</a>\r\n      </li>\r\n    </ul>\r\n\r\n    <!-- Right navbar links -->\r\n    <ul class=\"navbar-nav ml-auto\">\r\n      <!-- Navbar Search -->\r\n      \r\n    </ul>\r\n  </nav>\r\n  <!-- /.navbar -->\r\n\r\n  <!-- Main Sidebar Container -->\r\n  <aside class=\"main-sidebar sidebar-dark-primary elevation-4\">\r\n    <!-- Brand Logo -->\r\n    <a href=\"../index.html\" class=\"brand-link\">\r\n      <img src=\"../dist/img/AdminLTELogo.png\" alt=\"AdminLTE Logo\" class=\"brand-image img-circle elevation-3\" style=\"opacity: .8\">\r\n      <span class=\"brand-text font-weight-light\">Online Ombor</span>\r\n    </a>\r\n\r\n    <!-- Sidebar -->\r\n    <div class=\"sidebar\">\r\n      <!-- Sidebar user panel (optional) -->\r\n      <div class=\"user-panel mt-3 pb-3 mb-3 d-flex\">\r\n        <div class=\"image\">\r\n          <img src=\"../dist/img/user2-160x160.jpg\" class=\"img-circle elevation-2\" alt=\"User Image\">\r\n        </div>\r\n        <div class=\"info\">\r\n          <a href=\"#\" class=\"d-block\">Felix</a>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Sidebar Menu -->\r\n      <nav class=\"mt-2\">\r\n        <ul class=\"nav nav-pills nav-sidebar flex-column\" data-widget=\"treeview\" role=\"menu\" data-accordion=\"false\">\r\n          <li class=\"nav-header\">Menu</li>\r\n          <li class=\"nav-item\">\r\n            <a href=\"{% url 'stats' %}\" class=\"nav-link active\">\r\n              <i class=\"nav-icon far fa-circle text-warning\"></i>\r\n              <p>Statistics</p>\r\n            </a>\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <a href=\"{% url 'clientlar' %}\" class=\"nav-link\">\r\n              <i class=\"nav-icon far fa-circle text-danger\"></i>\r\n              <p class=\"text\">Clients</p>\r\n            </a>\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <a href=\"{% url 'mahsulotlar' %}\" class=\"nav-link\">\r\n              <i class=\"nav-icon far fa-circle text-info\"></i>\r\n              <p>Products</p>\r\n            </a>\r\n          </li>\r\n        </ul>\r\n      </nav>\r\n      <!-- /.sidebar-menu -->\r\n    </div>\r\n    <!-- /.sidebar -->\r\n  </aside>\r\n\r\n  <!-- Content Wrapper. Contains page content -->\r\n  <div class=\"content-wrapper\">\r\n    <!-- Content Header (Page header) -->\r\n    <div class=\"content-header\">\r\n      <div class=\"container-fluid\">\r\n        <div class=\"row mb-2\">\r\n          <div class=\"col-sm-6\">\r\n            <h1 class=\"m-0\">Stats</h1>\r\n          </div><!-- /.col -->\r\n          <div class=\"col-sm-6\">\r\n            <ol class=\"breadcrumb float-sm-right\">\r\n              <li class=\"breadcrumb-item\"><a href=\"#\">Home</a></li>\r\n              <li class=\"breadcrumb-item active\">Clients</li>\r\n                <li class=\"breadcrumb-item\"><a href=\"{% url 'logout' %}\">Logout</a></li>\r\n            </ol>\r\n          </div><!-- /.col -->\r\n        </div><!-- /.row -->\r\n      </div><!-- /.container-fluid -->\r\n    </div>\r\n    <!-- /.content-header -->\r\n    <div class=\"col-md-12\">\r\n        <div class=\"card card-dark\">\r\n            <div class=\"card-header border-0\">\r\n                <div class=\"d-flex justify-content-between\">\r\n                  <button type=\"submit\" class=\"btn btn-primary\" data-toggle=\"modal\" data-target=\"#myModal\">Statistika qo'shish</button>\r\n                </div>\r\n            </div>\r\n            <div class=\"card-body\">\r\n                <table class=\"table\" style=\"text-align: center;\">\r\n                    <thead>\r\n                        <tr>\r\n                            <th scope=\"col\" style=\"width: 15px\">T/r</th>\r\n                            <th scope=\"col\" style=\"width: 255px; text-align: center;\">Product</th>\r\n                            <th scope=\"col\" style=\"text-align: center;\">Client</th>\r\n                            <th scope=\"col\" style=\"text-align: center;\">Sana</th>\r\n                            <th scope=\"col\" style=\"text-align: center;\">Miqdor</th>\r\n                            <th scope=\"col\" style=\"text-align: center;\">Umumiy summa</th>\r\n                            <th scope=\"col\" style=\"text-align: center;\">To'landi</th>\r\n                            <th scope=\"col\" style=\"text-align: center;\">Nasiya</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                    {% for i in all_stats %}\r\n                        <tr>\r\n                            <th scope=\"row\">{{i.id}}</th>\r\n                            <td>{{i.product.nom}} {{i.product.brend_nomi}}</td>\r\n                            <td>{{i.client.ism}} {{i.client.dokon_nomi}}</td>\r\n                            <td>{{i.sanasi}}</td>\r\n                            <td>{{i.miqdor}}</td>\r\n                            <td>{{i.umumiy_summa}} so'm</td>\r\n                            <td>{{i.tolandi}} so'm</td>\r\n                            <td>{{i.nasiya}} so'm</td>\r\n                            <td><a href=\"{% url 'update' i.id %}\"><button type=\"submit\" class=\"btn btn-danger\" data-toggle=\"modal\">Tahrirlash</button></a></td>\r\n                        </tr>\r\n                    {% endfor %}\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    \r\n    <!-- The Modal -->\r\n    <div class=\"modal\" id=\"myModal\">\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content\">\r\n    \r\n          <!-- Modal Header -->\r\n          <div class=\"modal-header bg-dark\">\r\n            <h4 class=\"modal-title\">Yangi statistika</h4>\r\n            <button type=\"button\" class=\"close\" data-dismiss=\"modal\">&times;</button>\r\n          </div>\r\n    \r\n          <!-- Modal body -->\r\n          <form action=\"{% url 'stats' %}\" id=\"new-product-form\" method=\"post\">{% csrf_token %}\r\n            <div class=\"modal-body\">\r\n              <label for=\"product\">Product:</label>\r\n             <select name=\"product\" id=\"product\">\r\n                    {% for p in products %}\r\n                    <option value=\"{{p.id}}\">{{p.nom}} ({{p.brend_nomi}}, {{p.sotuvdagi_narxi}} so'm)</option>\r\n                    {% endfor %}\r\n                </select>\r\n              <br>\r\n              <label for=\"client\">Client:</label>\r\n                <select name=\"client\" id=\"client\">\r\n                    {% for c in clients %}\r\n                    <option value=\"{{c.id}}\">{{c.ism}} ({{c.dokon_nomi}}, {{c.joylashuv}})</option>\r\n                    {% endfor %}\r\n                </select>\r\n              <br>\r\n              <label for=\"sana\">Sana:</label>\r\n              <input type=\"datetime-local\" name=\"sana\" placeholder=\"Sana\" id=\"sana\" class=\"form-control\" required>\r\n              <br>\r\n                <label for=\"miqdor\">Miqdor:</label>\r\n              <input type=\"number\" name=\"miqdor\" placeholder=\"Miqdor\" id=\"miqdor\" class=\"form-control\" required>\r\n              <br>\r\n              <label for=\"umumiy_summa\">Umumiy summa:</label>\r\n              <input type=\"number\" name=\"summa\" placeholder=\"Umumiy summa\" id=\"umumiy_summa\" class=\"form-control\" required>\r\n              <br>\r\n                <label for=\"nasiya\">Nasiya:</label>\r\n              <input type=\"number\" name=\"nasiya\" placeholder=\"Nasiya\" id=\"nasiya\" class=\"form-control\" required>\r\n              <br>\r\n                <label for=\"tolandi\">To'landi:</label>\r\n              <input type=\"number\" name=\"tolandi\" placeholder=\"To'landi\" id=\"tolandi\" class=\"form-control\" required>\r\n              <br>\r\n            </div>\r\n    \r\n          <!-- Modal footer -->\r\n          <div class=\"modal-footer\">\r\n            <button type=\"submit\" class=\"btn btn-success\">Qo`shish</button>\r\n            <button type=\"button\" class=\"btn btn-danger\" data-dismiss=\"modal\">Yopish</button>\r\n        </div>\r\n      </form>\r\n    \r\n        </div>\r\n      </div>\r\n    </div>\r\n<!-- ./wrapper -->\r\n\r\n<!-- REQUIRED SCRIPTS -->\r\n\r\n<!-- jQuery -->\r\n<script src=\"{% static 'plugins/jquery/jquery.min.js' %}\"></script>\r\n<!-- Bootstrap -->\r\n<script src=\"{% static 'plugins/bootstrap/js/bootstrap.bundle.min.js' %}\"></script>\r\n<!-- AdminLTE -->\r\n<script src=\"{% static 'dist/js/adminlte.js' %}\"></script>\r\n\r\n<!-- OPTIONAL SCRIPTS -->\r\n<script src=\"{% static 'plugins/chart.js/Chart.min.js' %}\"></script>\r\n<!-- AdminLTE for demo purposes -->\r\n<script src=\"{% static 'dist/js/demo.js' %}\"></script>\r\n<!-- AdminLTE dashboard demo (This is only for demo purposes) -->\r\n<script src=\"{% static 'dist/js/pages/dashboard3.js' %}\"></script>\r\n</body>\r\n</html>\r\n"
        }
    ]
}